<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="sportPlayer">
	<select id="VideoTest" resultType="sportPlayerVO">
		select * from SPORT_PLAYER
	</select>
	<select id="VideoDetail" resultType="sportPlayerVO">
		select * from SPORT_PLAYER where videoNum = #{videoNum}
	</select>
</mapper>
